{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    },
    "FileLogger": {
      "Options": {
        "FolderPath": "C:\\logs",
        "FilePath": "log_{date}.log"
      },
      "EventLog": {
        "LogLevel": {
          "Microsoft": "Information",
          "Microsoft.Hosting.Lifetime": "Information"
        }
      }
    }
  }
}